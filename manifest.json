{
  "manifest": "1",
  "client_id": "{{YOUR_CLIENT_ID}}",
  "version": "1.0.0",
  "callback_url": "https://social-icons-php.herokuapp.com/oauth/phase-one", 
  "oauth_final_destination": "home",
  "webhooks": {
    "callback_url": "https://social-icons-php.herokuapp.com/webhooks/callback",
    "events": [
      "user.update","site.publish", "site.delete", "app.uninstall"
    ]
  },
  "scopes": [
    "read:user",
    "read:site"
  ],
  "locale": {
    "default": "en-us",
    "supported": ["en-us"]
  }
}
